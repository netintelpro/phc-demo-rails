<h1>Listing profiles</h1>

<table>
  <tr>
    <th>Street1</th>
    <th>Street1</th>
    <th>City</th>
    <th>State</th>
    <th>Zip</th>
    <th>Phone</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @profiles.each do |profile| %>
  <tr>
    <td><%= profile.street1 %></td>
    <td><%= profile.street1 %></td>
    <td><%= profile.city %></td>
    <td><%= profile.state %></td>
    <td><%= profile.zip %></td>
    <td><%= profile.phone %></td>
    <td><%= link_to 'Show', profile %></td>
    <td><%= link_to 'Edit', edit_profile_path(profile) %></td>
    <td><%= link_to 'Destroy', profile, :method => :delete, :data => { :confirm => 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

